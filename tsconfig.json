{
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": ["es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  },
  "exclude": [],
  "include": ["source/**/*.ts"]
}
